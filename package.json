{
  "name": "livetl.app",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "prepare": "svelte-kit sync",
    "dev": "vite dev --host",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "type:check": "svelte-check --tsconfig ./tsconfig.json",
    "type:check:watch": "svelte-check --watch --tsconfig ./tsconfig.json",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@octokit/core": "^4.2.4",
    "daisyui": "^2.52.0",
    "svelte-inview": "^4.0.4",
    "sveltekit-i18n": "^2.4.2"
  },
  "devDependencies": {
    "@steeze-ui/heroicons": "^2.4.2",
    "@steeze-ui/radix-icons": "^1.1.2",
    "@steeze-ui/simple-icons": "^1.10.1",
    "@steeze-ui/svelte-icon": "^1.6.2",
    "@sveltejs/adapter-auto": "^3.3.1",
    "@sveltejs/adapter-cloudflare": "^7.0.2",
    "@sveltejs/enhanced-img": "^0.3.10",
    "@sveltejs/kit": "^2.20.8",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@typescript-eslint/eslint-plugin": "^8.32.0",
    "@typescript-eslint/parser": "^8.32.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-json-format": "^2.0.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-svelte": "^2.46.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "prettier-plugin-svelte": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "svelte": "^5.28.2",
    "svelte-check": "^4.1.7",
    "svelte-eslint-parser": "^0.42.0",
    "svelte-i18n": "^4.0.1",
    "svelte-meta-tags": "^4.3.0",
    "svelte-preprocess": "^6.0.3",
    "tailwindcss": "^3.4.17",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3",
    "vite": "^6.3.5"
